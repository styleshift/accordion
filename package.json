{
  "name": "@styleshift/accordion",
  "version": "0.0.1",
  "description": "Accordion for StyleShift UI",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
